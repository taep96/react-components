{
  "name": "react-components",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "postlint": "manypkg check",
    "lint:fix": "turbo lint:fix",
    "postlint:fix": "manypkg fix",
    "typecheck": "turbo typecheck",
    "format": "prettier . !pnpm-lock.yaml --ignore-path .gitignore --write --check --cache",
    "publish-packages": "pnpm build && pnpm changeset publish"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@types/eslint": "^8.37.0",
    "@types/node": "^20.2.1",
    "@types/prettier": "^2.7.2",
    "eslint": "^8.41.0",
    "eslint-config-custom": "*",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "turbo": "^1.9.8",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": "^18.0.0",
    "pnpm": "^8.0.0"
  },
  "packageManager": "pnpm@8.5.1"
}
