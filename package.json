    "eslint": "^8.52.0",
    "eslint-config-custom": "*",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "^18.0.0",
